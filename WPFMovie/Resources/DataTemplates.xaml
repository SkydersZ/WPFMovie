<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodels="clr-namespace:WPFMovie.ViewModels"
                    xmlns:views="clr-namespace:WPFMovie.Views">
    
    <DataTemplate x:Key="ViewModelAllMoviesTemplate" DataType="{x:Type viewmodels:ViewModelAllMovies}">
        <views:ViewAllMovies/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:ViewModelMyMovies}">
        <views:ViewMyMovies/>
    </DataTemplate>
</ResourceDictionary>