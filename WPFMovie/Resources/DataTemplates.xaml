<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodels="clr-namespace:WPFMovie.ViewModels"
                    xmlns:views="clr-namespace:WPFMovie.Views">
    
    <DataTemplate DataType="{x:Type viewmodels:ViewModelMovies}">
        <views:MoviesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:ViewModelMyMovies}">
        <views:MyMoviesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:ViewModelSearch}">
        <views:SearchView/>
    </DataTemplate>
    

</ResourceDictionary>